<!-- incorporate search and top 4 songs by number of likes -->
<div id="top_songs">

</div>

<div id="recent_songs">
  <h1>Recent Songs</h1>
  <% @songs.reverse.each do |s| %>
    <a style="color: ghostwhite;" href="/songs/<%= s.id %>"><%= s.artist %> - "<%= s.title %>" <span><%= s.likes.count %> Likes</span><br/><br/>
  <% end %>
</div>
